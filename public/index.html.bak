    <title>Cyber Smart Pros</title>

    <link rel="shortcut icon" type="image/x-icon" href="images/logos/Cyber-Smart-Pros-logo.png">
    <link href="css/libs.min.css" rel="stylesheet">
    <link href="css/styles.min.css" rel="stylesheet">

    <script src="js/libs.min.js"></script>
    <script src="js/bundle.js"></script>
</head>

<body ng-app="BusinessSite">

    <div class="page-wrapper">

        <!-- Header -->
        <nav class="navbar navbar-default navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false"
                        aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>

                        <i class="material-icons closeIcon">close</i>
                    </button>
                    <a class="navbar-brand" href="index.html">
                        <span class="img-logo"><img src="images/logos/Cyber-Smart-Pros-logo.png"></span> 
                        <span class="title-logo">Cyber Smart Pros</span>
                    </a>
                </div>

                <div id="navbar" class="navbar-collapse collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Our Company <span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a href="aboutus.html">About Us</a></li>
                                <li><a href="aboutus.html#mission-vision-value">Mission, Vision, & Values</a></li>
                                <li><a href="executiveTeam.html">Executive Team</a></li>
                            </ul>
                        </li>
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Capabilities <span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a href="capabilities.html#advisory">Advisory</a></li>
                                <li><a href="capabilities.html#cyber">Cyber Security</a></li>
                                <li><a href="capabilities.html#software">Software Development</a></li>
                            </ul>
                        </li>
                        <!-- <li><a href="capabilities.html">Capabilities</a></li> -->
                        <li><a href="clients.html">Clients</a></li>
                        <li><a href="joinOurTeam.html">Join Our Team</a></li>
                        <li><a href="contactus.html">Contact Us</a></li>
                    </ul>
                </div>
            </div>
        </nav>

        <div class="mainBody">
            <div class="content-body home" ng-controller="HomePage as cc">
    <!-- Home Carousel -->
    <div class="carousel-container">
        <div class="back-title"><img src="images/logos/Cyber-Smart-Pros-logo.png"><div>Cyber Smart Pros</div></div>        
        <ui-carousel slides="cc.slides" slides-to-scroll="1" autoplay="true" autoplay-speed="7000" dots="false">
            <carousel-item>                
                <div class="item-image">
                    <div class="texture-overlay"></div>
                    <div class="color-overlay"></div>
                    <img src="{{item.img}}">
                </div>
                <div class="cover-text-box">
                    <div class="box-title">{{item.title}}</div>
                    <p class="box-text">{{item.text}}</p>
                </div>
            </carousel-item>

            <carousel-prev>
                <i class="material-icons">chevron_left</i>
            </carousel-prev>

            <carousel-next>
                <i class="material-icons">chevron_right</i>
            </carousel-next>
        </ui-carousel>
    </div>
    
    <!-- Section 1 -->
    <div class="content-container">
        <div class="row-layout" layout="row" layout-xs="column">
            <div class="row-column" flex>
                
                <!-- Computer -->
                <div class="css-object css-computer screen">
                    <div class="content">
                        <div class="pg">
                        <ul class="btns">
                            <li ng-repeat="i in cc.getNumber(3) track by $index"></li>
                        </ul>
                        <ul class="txt">
                            <li ng-repeat="i in cc.getNumber(2) track by $index"></li>
                            <li class="thin" ng-repeat="i in cc.getNumber(3) track by $index"></li>
                        </ul>
                        </div>
                    </div>
                    <div class="base">
                        <div class="grey-shadow"></div>
                        <div class="foot top"></div>
                        <div class="foot bottom"></div>
                        <div class="shadow"></div>
                        <div class="keyboard">
                        <div class="btm"></div>
                        <ul class="keys">
                            <li ng-repeat="i in cc.getNumber(4) track by $index"></li>
                        </ul>
                        <div class="shadow"></div>
                        </div>
                    </div>
                </div>

                <!-- Mobile -->
                <div class="css-object css-phone">
                    <div class="screen">
                        <div class="content">
                        <ul class="txt">
                            <li ng-repeat="i in $ctrl.getNumber(3) track by $index"></li>
                        </ul>
                        </div>
                    </div>

                    <div class="shadow"></div>
                </div>
            </div>
            <div class="row-column" flex>
                <div class="content-title add-line">
                    <div class="title-txt">Some Words About Us</div>
                    <div class="line"><hr></div>
                </div>
                <div class="content-text">
                    We provide information technology and management consulting services to federal, state, and local governments as well as private sector companies. Our core competencies include advisory, cybersecurity, and software development services. We are a minority-owned business and a verified Service Disabled Veteran Owned Small Business (SDVOSB).
                </div>
            </div>
        </div>
    </div>
    <!-- Section 2 -->
    <div class="content-container">
        <div class="background-img-container">
            <div class="texture-overlay"></div>
            <div class="gradient-overlay"></div>
            <div class="parallax-backimg" back-img={{cc.dividerImg}}></div>
        </div>
        <div class="service-area-container">
            <div class="content-title"><div class="title-txt">Our Services</div></div>
            <div class="row-layout" layout="row" layout-xs="column">
                <div class="row-column" flex ng-repeat="service in cc.services">
                    <a href="{{ service.url }}" class="service-container">
                        <div class="service-icon"><i class="material-icons">{{service.icon}}</i></div>
                        <div class="service-title">{{ service.title}}</div>
                        <div class="service-text">{{ service.text}}</div>
                    </a>
                </div>            
            </div>
        </div>
    </div>
    <!-- Section 3 -->
    <div class="content-container">
        <div class="content-title add-line">
            <div class="title-txt">Latest News</div>
            <div class="line"><hr></div>
        </div>
        <div class="news-carousel-container">        
            <ui-carousel slides="cc.latestNews" slides-to-scroll="1" autoplay="true" autoplay-speed="7000" dots="false">
                <carousel-item>                                    
                    <div class="news-text-box">                        
                        <p class="news-text"><span>"</span>{{item.text}} ...<span>"</span></p>
                        <p class="new-date">{{ item.date | date: 'MMMM, yyyy'  }}</p>
                    </div>
                </carousel-item>

                <carousel-prev>
                    <i class="material-icons">chevron_left</i>
                </carousel-prev>

                <carousel-next>
                    <i class="material-icons">chevron_right</i>
                </carousel-next>
            </ui-carousel>
        </div>
    </div>
</div>
        </div>
        <div class="push"></div>
    </div>

    <div class="bottom">
        <!-- footer -->
        <div class="footer-container">
            <div class="main-section row-layout" layout="row" layout-xs="column">
                <div class="row-column" flex>
                    <div class="sub-row">Feedback is the lifeblood of our business.</div>
                    <div class="sub-row">Please contact us at:</div>
                    <div class="sub-row">703-678-9053</div>
                    <a id="footer-email" class="sub-row" href="#">info@cybersmartpros.com</a>
                </div>
                <div class="row-column" flex>
                    <div class="sub-row"><a href="index.html">Home</a></div>
                    <div class="sub-row"><a href="aboutus.html">About Us</a></div>
                    <div class="sub-row"><a href="aboutus.html#mission-vision-value">Mission, Vision, Values</a></div>
                    <div class="sub-row"><a href="executiveTeam.html">Our Leadership</a></div>
                </div>
                <div class="row-column" flex>
                    <div class="sub-row"><a href="capabilities.html">Capabilities & Services</a></div>
                    <div class="sub-row"><a href="clients.html">Clients</a></div>
                    <div class="sub-row"><a href="joinOurTeam.html">Join Our Team</a></div>
                    <div class="sub-row"><a href="contactus.html">Contact Us</a></div>
                </div>
            </div>
            <div class="sub-section">
                <div class="row-layout" layout="row" layout-xs="column">
                    <div class="row-column left" flex>CYBER SMART PROS © COPYRIGHT 2017</div>
                    <div class="row-column right" flex>SITE BY <a href="">DWJ PRODUCTION WEB</a></div>
                </div>
            </div>
        </div>
    </div>
</body>

<script>
        var link = document.getElementById('footer-email');
        link.onclick = function () {
            this.href = "mailto:info@cybersmartpros.com?subject=Data&body=cybersmartpros.com";
        };
</script>

</html>